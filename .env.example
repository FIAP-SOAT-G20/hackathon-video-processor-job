# Video Processing Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# REQUIRED VARIABLES
# =============================================================================

# S3 Key (path) of the video file to process
VIDEO_KEY=videos/sample.mp4

# S3 bucket for input video files
VIDEO_BUCKET=video-processor-raw-videos

# S3 bucket for processed output files (ZIP with extracted frames)
K8S_JOB_ENV_PROCESSED_BUCKET=video-processor-processed-images

# =============================================================================
# OPTIONAL VARIABLES
# =============================================================================

# Output format for extracted frames (jpg or png)
# Default: jpg
K8S_JOB_ENV_VIDEO_EXPORT_FORMAT=jpg

# Frame extraction rate (frames per second)
# Default: 1.0
K8S_JOB_ENV_VIDEO_EXPORT_FPS=1.0

# =============================================================================
# AWS CREDENTIALS
# =============================================================================

# AWS Access Key ID
K8S_JOB_ENV_AWS_ACCESS_KEY_ID=your-access-key-id

# AWS Secret Access Key
K8S_JOB_ENV_AWS_SECRET_ACCESS_KEY=your-secret-access-key

# AWS Session Token (required for temporary credentials)
K8S_JOB_ENV_AWS_SESSION_TOKEN=your-session-token

# AWS Region
K8S_JOB_ENV_AWS_REGION=us-east-1

