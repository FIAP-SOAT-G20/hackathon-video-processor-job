# Video Processing Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# REQUIRED VARIABLES
# =============================================================================

# S3 Key (path) of the video file to process
VIDEO_KEY=videos/sample.mp4

# S3 bucket for input video files
VIDEO_BUCKET=video-processor-raw-videos

# S3 bucket for processed output files (ZIP with extracted frames)
PROCESSED_BUCKET=video-processor-processed-images

# =============================================================================
# OPTIONAL VARIABLES
# =============================================================================

# Output format for extracted frames (jpg or png)
# Default: jpg
VIDEO_EXPORT_FORMAT=jpg

# Frame extraction rate (frames per second)
# Default: 1.0
VIDEO_EXPORT_FPS=1.0

# =============================================================================
# AWS CREDENTIALS
# =============================================================================

# AWS Access Key ID
AWS_ACCESS_KEY_ID=your-access-key-id

# AWS Secret Access Key
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# AWS Session Token (required for temporary credentials)
AWS_SESSION_TOKEN=your-session-token

# AWS Region
AWS_REGION=us-east-1

