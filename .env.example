# Video Processing Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# REQUIRED VARIABLES
# =============================================================================

# S3 Key (path) of the video file to process
K8S_JOB_ENV_VIDEO_KEY=videos/sample.mp4

# S3 bucket for input video files
K8S_JOB_ENV_VIDEO_BUCKET=video-processor-raw-videos

# S3 bucket for processed output files (ZIP with extracted frames)
K8S_JOB_ENV_PROCESSED_BUCKET=video-processor-processed-images

# =============================================================================
# OPTIONAL VARIABLES
# =============================================================================

# Output format for extracted frames (jpg or png)
# Default: jpg
K8S_JOB_ENV_VIDEO_EXPORT_FORMAT=jpg

# Frame extraction rate (frames per second)
# Default: 1.0
K8S_JOB_ENV_VIDEO_EXPORT_FPS=1.0

